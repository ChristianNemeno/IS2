  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "23f7a6f7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Found classes: ['Angry', 'Fear', 'Happy', 'Sad', 'Suprise']\n",
      "\n",
